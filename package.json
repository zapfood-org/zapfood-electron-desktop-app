{
  "name": "ZapFood",
  "private": true,
  "version": "1.0.5",
  "main": "dist-electron/main.js",
  "author": {
    "name": "Romulo G Donadoni",
    "email": "romulogdonadoni@gmail.com"
  },
  "description": "ZapFood Desktop App",
  "build": {
    "artifactName": "ZapFood-Setup-${version}.${ext}",
    "appId": "com.zapfood.desktop",
    "productName": "ZapFood",
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "zapfood-org",
        "repo": "desktop-releases"
      }
    ],
    "icon": "src/assets/images/zappyfood.png",
    "win": {
      "target": "nsis",
      "icon": "src/assets/images/zappyfood.png"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@heroui/drawer": "^2.2.24",
    "@heroui/react": "^2.8.5",
    "@iconify/react": "^6.0.2",
    "@origin-space/image-cropper": "^0.1.9",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@sinclair/typebox": "^0.34.45",
    "@solar-icons/react": "^1.0.1",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.561.0",
    "moment": "^2.30.1",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^7.10.1",
    "react-toastify": "^11.0.5",
    "recharts": "^3.5.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
}